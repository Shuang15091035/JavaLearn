<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.jpym.ymfrontgm.dao.FundPerformanceDao">
    <resultMap id="FundPerformanceModel" type="com.jpym.ymfrontgm.model.FundPerformanceModel">
        <result property="fundcode" column="fund_code" jdbcType="VARCHAR"></result>
        <result property="fundname" column="fund_name" jdbcType="VARCHAR"></result>
        <result property="nav1m" column="nav1m" jdbcType="FLOAT"></result>
        <result property="nav3m" column="nav3m" jdbcType="FLOAT"></result>
        <result property="nav6m" column="nav6m" jdbcType="FLOAT"></result>
        <result property="nav1y" column="nav1y" jdbcType="FLOAT"></result>
        <result property="annualyieldl7d" column="annualyieldl7d" jdbcType="FLOAT"></result>
        <result property="annualyieldl1m" column="annualyieldl1m" jdbcType="FLOAT"></result>
        <result property="annualyieldl3m" column="annualyieldl3m" jdbcType="FLOAT"></result>
        <result property="annualyieldl6m" column="annualyieldl6m" jdbcType="FLOAT"></result>
        <result property="yieldt1y" column="yieldt1y" jdbcType="FLOAT"></result>
        <result property="yieldl2y" column="yieldl2y" jdbcType="FLOAT"></result>
        <result property="yieldl3y" column="yieldl3y" jdbcType="FLOAT"></result>
        <result property="yieldl5y" column="yieldl5y" jdbcType="FLOAT"></result>
        <result property="yieldfound" column="yieldfound" jdbcType="VARCHAR"></result>
        <result property="similarrankl1m" column="similarRankL1m" jdbcType="VARCHAR"></result>
        <result property="similarrankl3m" column="similarRankL3m" jdbcType="VARCHAR"></result>
        <result property="similarrankl6m" column="similarRankL6m" jdbcType="VARCHAR"></result>
        <result property="similarRankL1y" column="similarRankL1y" jdbcType="VARCHAR"></result>
    </resultMap>

    <select id="searchFundPerformance" parameterType="String" resultMap="FundPerformanceModel">
        SELECT tfp.fund_code AS fund_code,
        tfp.fund_name AS fund_name,
        tfp.nav_1m AS nav1m, --最近1月净值增长率 NUMERIC(19,6) %
        tfp.nav_3m AS nav3m, --最近3月净值增长率 NUMERIC(19,6) %
        tfp.nav_6m AS nav6m, --最近6月净值增长率 NUMERIC(19,6) %
        tfp.nav_1y as nav1y, --最近1年净值增长率 NUMERIC(19,6) %
        tfp.annual_yield_l7d AS annualyieldl7d, --最近7日年化收益率
        tfp.annual_yield_l1m AS annualyieldl1m, --最近1月年化收益率
        tfp.annual_yield_l3m AS annualyieldl3m, --最近3月年化收益率
        tfp.annual_yield_l6m AS annualyieldl6m, --最近6月年化收益率
        tfp.yield_t1y AS yieldt1y, --最近1年的年化收益率
        tfp.yield_l2y AS yieldl2y, --最近2年的年化收益率
        tfp.yield_l3y AS yieldl3y, --最近3年的年化收益率
        tfp.yield_l5y AS yieldl5y, --最近5年的年化收益率
        tfp.yield_found AS yieldfound, --成立以来的年化收益率
        tfp.similar_rank_l1m AS similarRankL1m,
        tfp.similar_rank_l3m AS similarRankL3m,
        tfp.similar_rank_l6m AS similarRankL6m,
        tfp.similar_rank_l1y AS similarRankL1y --同类型排名(近1年)
        FROM t_fund_performance tfp
        WHERE tfp.fund_code = #{fund_code}
    </select>

</mapper>