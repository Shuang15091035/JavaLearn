<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.jpym.ymfrontgm.dao.FundStrategyDao">

    <resultMap id="FundStrategyModel" type="com.jpym.ymfrontgm.model.FundStrategyModel">
        <result property="fundcode" column="fund_code" jdbcType="VARCHAR"></result>
        <result property="fundname" column="fund_name" jdbcType="VARCHAR"></result>
        <result property="investpolicy" column="invest_policy" jdbcType="VARCHAR"></result>
    </resultMap>

    <select id="searchFundStrategy" parameterType="String" resultMap="FundStrategyModel">
        SELECT ftfbf.FSYMBOL AS fund_code, --基金代码
        ftfbf.FDSNAME AS fund_name, --基金名称
        to_char(ftfbf.INVESTPOLICY) AS invest_policy --投资策略
        FROM finchina.tq_fd_basicInfo@finchinadb ftfbf --基金基本信息
        WHERE ftfbf.FSYMBOL = #{fund_code}
    </select>
</mapper>