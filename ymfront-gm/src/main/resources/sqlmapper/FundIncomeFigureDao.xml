<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.jpym.ymfrontgm.dao.FundIncomeFigureDao">
    <!--###产品收益走势-->
    <resultMap id="FundIncomeFigureModel" type="com.jpym.ymfrontgm.model.FundIncomeFigureModel">
        <result property="fundcode" column="fund_code" jdbcType="VARCHAR"></result>
        <result property="fundname" column="fund_name" jdbcType="VARCHAR"></result>
        <result property="dayincrease" column="day_increase" jdbcType="VARCHAR"></result>
        <result property="tradedate" column="trade_date" jdbcType="VARCHAR"></result>
    </resultMap>

    <select id="searchFundIncomeFigure" resultMap="FundIncomeFigureModel">
        SELECT tft.fund_code AS fund_code, --基金代码
        tft.fund_name AS fund_name, --基金名称
        tft.day_increase AS day_increase, --日涨跌幅 %
        tft.trade_date AS trade_date --交易日期
        FROM t_fund_trend tft --产品收益走势表
        WHERE tft.trade_date BETWEEN #{start_date} AND #{end_date}
        AND tft.fund_code = #{fund_code}
        ORDER BY tft.trade_date
    </select>

</mapper>