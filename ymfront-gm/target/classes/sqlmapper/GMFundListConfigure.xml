<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.jpym.ymfrontgm.dao.GMFundListConfigureDao">
    <resultMap id="FundListConfigureModel" type="com.jpym.ymfrontgm.model.GMFundListConfigureModel">
        <result property="fundcode" column="fundcode" jdbcType="VARCHAR"></result>
        <result property="labeltype" column="labeltype" jdbcType="VARCHAR"></result>
        <result property="yearlimit" column="yearlimit" jdbcType="VARCHAR"></result>
        <result property="fundstarlevel" column="fundstarlevel" jdbcType="VARCHAR"></result>
        <result property="funddescribe" column="funddescribe" jdbcType="VARCHAR"></result>
    </resultMap>
    <select id="queryFundListConfigureFile" resultMap="FundListConfigureModel">
        SELECT tgpc.FUND_CODE AS fundcode,
        tgpc.LABEL_TYPE AS labeltype,
        tgpc.year_limit AS yearlimit,
        tgpc.fund_star AS fundstarlevel,
        TO_CHAR(tgpc.fund_describe) AS funddescribe
        FROM T_GM_PRODUCT_CONFIG tgpc
    </select>
</mapper>